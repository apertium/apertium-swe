LIST Jag = "jag" ; 
LIST Sex = "sex" ; 

LIST Kunna = "kunna" ;
LIST Vilja = "vilja" ;

LIST Var = "<var>"i ;

SET Aux = Kunna | Vilja ;

SET Fin = Pres | Past ;

###############################################################################
#  Morphological disambiguation
#

SECTION

SELECT Prn IF (0 Jag) ;

SELECT Num IF (0 Sex) (1 N) ; 

SELECT Interr IF (0 Var) (1 Fin) ;

SELECT V IF (0 Var) (NOT 1 Fin) ;

REMOVE Det IF (1 Fin) ;

SELECT Past IF (0 Var) (-1 N OR Prn) ;

SELECT Inf IF (-1 Aux) ;


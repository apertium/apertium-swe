LIST Kunna = "kunna" ;
LIST Vilja = "vilja" ;
LIST Ha = "ha" ;

LIST Sg = sg ; 
LIST Pl = pl ;
LIST Sp = sp ;

LIST Var = "<var>"i ;

SET Aux = Kunna | Vilja ;

SET Fin = Pres | Past ;

SET Number = Pl | Sg ;

###############################################################################
#  Morphological disambiguation
#

SECTION

SELECT Prn IF (0 Jag) ;

SELECT Num IF (0 Sex) (1 N) ; 

SELECT Interr IF (0 Var) (1 Fin) ;

SELECT V IF (0 Var) (NOT 1 Fin) ;

REMOVE Det IF (1 Fin) ;

SELECT Past IF (0 Var) (-1 N OR Prn) ;

SELECT Inf IF (-1 Aux) ;

REMOVE Def IF (0 Prop) ;

SELECT Supn IF (-1* Ha BARRIER (*) - Adv) ;

SELECT N + $$Number IF (-1 A + $$Number) (0 N) ;


